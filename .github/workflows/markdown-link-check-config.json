{
  "ignorePatterns": [
    {
      "pattern": "^https://github\\.com/yourusername/"
    },
    {
      "pattern": "^http://localhost"
    },
    {
      "pattern": "^http://192\\.168\\."
    },
    {
      "pattern": "^http://127\\.0\\.0\\.1"
    }
  ],
  "replacementPatterns": [
    {
      "pattern": "^/",
      "replacement": "{{BASEURL}}/"
    }
  ],
  "httpHeaders": [
    {
      "urls": ["https://github.com"],
      "headers": {
        "Accept-Encoding": "zstd, br, gzip, deflate"
      }
    }
  ],
  "timeout": "20s",
  "retryOn429": true,
  "retryCount": 3,
  "fallbackHttpStatus": [200, 201, 204, 301, 302, 307, 308]
}
